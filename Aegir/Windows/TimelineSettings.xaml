<Window x:Class="Aegir.Windows.TimelineSettings"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Aegir.Windows"
        mc:Ignorable="d"
        Title="Timeline Settings" Height="300" Width="300">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
            <RowDefinition Height="25"></RowDefinition>
        </Grid.RowDefinitions>
        <GroupBox Header="Playback">
        </GroupBox>
        <GroupBox Grid.Row="1" Header="Timeline">
        </GroupBox>
        <Button Grid.Row="2">Ok</Button>
    </Grid>
</Window>