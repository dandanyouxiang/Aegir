<UserControl x:Class="Aegir.View.Timeline.KeyframeTimeline"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Aegir.View.Timeline"
             mc:Ignorable="d" 
             d:DesignHeight="32" d:DesignWidth="300"
             SizeChanged="UserControl_SizeChanged">
    <Grid>
        <Canvas SnapsToDevicePixels="True" Grid.Row="1" Height="20px" VerticalAlignment="Top" x:Name="KeyFrameTimeLineRuler">
        </Canvas >
        
    </Grid>
</UserControl>
